{
	"name": "website",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma-gen-logs": "pnpm exec prisma generate --schema=prisma/schema_logs.prisma",
		"prisma-setup-logs": "pnpm exec prisma db pull --schema=prisma/schema_logs.prisma && pnpm exec prisma generate --schema=prisma/schema_logs.prisma"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^5.2.5",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/node-steam-openid": "^1.0.3",
		"@types/ramda": "^0.29.9",
		"autoprefixer": "^10.4.16",
		"lucia": "^3.1.1",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@fireworks-js/svelte": "^2.10.8",
		"@supabase/auth-helpers-sveltekit": "^0.13.0",
		"@supabase/supabase-js": "^2.39.0",
		"arctic": "^1.4.0",
		"browser-image-compression": "^2.0.2",
		"directory-tree": "^3.5.1",
		"dotenv": "^16.4.5",
		"node-steam-openid": "^1.2.3",
		"oslo": "^1.1.3",
		"ramda": "^0.29.1",
		"resend": "^3.2.0",
		"skeleton-elements": "^4.0.1",
		"stripe": "^14.7.0",
		"svelte-easy-crop": "^3.0.0",
		"svelte-i18n": "^4.0.0",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	}
}
